NNTOOL=nntool
MODEL_SQ8=1
# MODEL_POW2=1
# MODEL_FP16=1
# MODEL_NE16=1

MODEL_SUFFIX?=
MODEL_PREFIX?=efficientnet_lite
MODEL_PYTHON=python3
MODEL_BUILD=BUILD_MODEL$(MODEL_SUFFIX)

MODEL_TYPE ?= 'onnx'
MODEL_SIZE ?= 0
ifeq '$(MODEL_TYPE)' 'tflite'
TRAINED_MODEL = models/efficientnet_lite$(MODEL_SIZE)_int8_2.tflite
NNTOOL_EXTRA_FLAGS += -q
NNTOOL_SCRIPT = models/nntool_script_q
else
MODEL_SIZE=4
TRAINED_MODEL = models/efficientnet-lite4.onnx
NNTOOL_SCRIPT = models/nntool_script
endif
MODEL_EXPRESSIONS = 


# Memory sizes for cluster L1, SoC L2 and Flash
ifeq '$(TARGET_CHIP_FAMILY)' 'GAP9'
	CLUSTER_NUM_CORES=8
	FREQ_CL?=370
	FREQ_FC?=370
	FREQ_PE?=370
	TARGET_L1_SIZE ?= 128000
	TARGET_L2_SIZE ?= 1300000
	TARGET_L3_SIZE ?= 8000000
else
	CLUSTER_NUM_CORES=7
	ifeq '$(TARGET_CHIP)' 'GAP8_V3'
		FREQ_CL?=175
	else
		FREQ_CL?=50
	endif
	FREQ_FC?=250
	FREQ_PE?=250
	TARGET_L1_SIZE ?= 64000
	TARGET_L2_SIZE ?= 400000
	TARGET_L3_SIZE ?= 8000000
endif

# Cluster stack size for master core and other cores
CLUSTER_STACK_SIZE=2048
CLUSTER_SLAVE_STACK_SIZE=512
ifeq ($(MODEL_NE16), 1)
	NNTOOL_SCRIPT := $(NNTOOL_SCRIPT)_ne16
	MODEL_SUFFIX = _NE16
	TRANSPOSE_2CHW = 0
	APP_CFLAGS += -DMODEL_NE16
else
ifeq ($(MODEL_HWC), 1)
	NNTOOL_SCRIPT := $(NNTOOL_SCRIPT)_hwc
	MODEL_SUFFIX = _HWC
	TRANSPOSE_2CHW = 0
else
	TRANSPOSE_2CHW = 1
endif
endif



$(info GEN ... $(CNN_GEN))
